#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_peer@0.2.10@peer/bin/peerjs" "$@"
  ret=$?
else 
  node  "$basedir/../_peer@0.2.10@peer/bin/peerjs" "$@"
  ret=$?
fi
exit $ret
